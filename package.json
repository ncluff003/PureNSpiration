{
  "name": "purenspiration",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "BundleJS": "webpack --config ./PureNSpiration/webpack.config.js --mode production",
    "CompileSass": "sass Public/SCSS/Main/main.scss:./Public/CSS/purenspiration.comp.css --style=expanded --embed-sources",
    "PrefixCSS": "postcss --use=autoprefixer -b \"last 20 versions\" ./Public/CSS/purenspiration.comp.css -o ./Public/CSS/purenspiration.prefix.css",
    "CompressCSS": "sass ./Public/CSS/purenspiration.prefix.css:./Public/DIST/CSS/purenspiration.css --style=compressed --embed-sources",
    "BuildCSS": "npm-run-all CompileSass PrefixCSS CompressCSS ",
    "BundleDevJS": "webpack --config ./webpack.config.js --mode development",
    "LocalServer": "nodemon Backend_JS/Server.js -e pug,js,css,html,jpg,png,scss,vue",
    "WatchSCSS": "onchange '**/*.scss' -- npm-run-all BuildCSS",
    "WatchJS": "onchange '**/*.js' -- npm-run-all BundleDevJS",
    "Start": "npm-run-all --parallel LocalServer WatchSCSS WatchJS",
    "start": "node Backend_JS/Server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ncluff003/PureNSpiration.git"
